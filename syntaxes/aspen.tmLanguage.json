{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "aspen",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.aspen",
          "match": "\\b(object|class|instance|of)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.aspen",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.aspen",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.aspen"
}
